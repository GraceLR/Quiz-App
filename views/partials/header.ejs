<header class="nav-header">
  <div class="q-icon">
    <a href="http://localhost:8080/"> <i class="fa-brands fa-quora"></i></a>
    </div>
  <nav>
    <!-- <a class="nav-item" id="login" href="/quiz/myquiz">Log in</a> -->

    <% if(!loggedInUser){ %>
      <form class="form" action="/session/in" method="POST">
        <button type="submit" class="btn btn-primary">Login</button>
      </form>
      <% } else { %>
        <% let name=undefined %>
          <% if(loggedInUser===1){ %>
            <% name='Alice' %>
              <% } else { %>
                <% name='Kira' %>
                  <% } %>
                    <form class="form-inline" action="/session/out" method="POST">
                      <label class="mr-3" for="Hi">Hi: <%= name %></label>
                      <button type="submit" class="btn btn-outline-dark">Logout</button>
                    </form>
                    <% } %>

  </nav>

</header>
